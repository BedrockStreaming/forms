{
  "version": 2,
  "projects": {
    "demo": {
      "root": "apps/demo",
      "sourceRoot": "apps/demo/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/demo",
            "index": "apps/demo/src/index.html",
            "main": "apps/demo/src/main.tsx",
            "polyfills": "apps/demo/src/polyfills.ts",
            "tsConfig": "apps/demo/tsconfig.app.json",
            "assets": ["apps/demo/src/favicon.ico", "apps/demo/src/assets"],
            "styles": ["apps/demo/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/demo/src/environments/environment.ts",
                  "with": "apps/demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "demo:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "demo:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/demo/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/demo"],
          "options": {
            "jestConfig": "apps/demo/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "demo-e2e": {
      "root": "apps/demo-e2e",
      "sourceRoot": "apps/demo-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/demo-e2e/cypress.json",
            "tsConfig": "apps/demo-e2e/tsconfig.e2e.json",
            "devServerTarget": "demo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/demo-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "docsite": {
      "projectType": "application",
      "root": "apps/docsite",
      "sourceRoot": "apps/docsite/src",
      "targets": {
        "build": {
          "executor": "@nx-plus/docusaurus:browser",
          "options": {
            "outputPath": "dist/apps/docsite"
          }
        },
        "serve": {
          "executor": "@nx-plus/docusaurus:dev-server",
          "options": {
            "port": 3000
          }
        }
      }
    },
    "examples-birthdate": {
      "root": "libs/examples/birthdate",
      "sourceRoot": "libs/examples/birthdate/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/examples/birthdate/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/examples/birthdate"],
          "options": {
            "jestConfig": "libs/examples/birthdate/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "examples-styled-inputs": {
      "root": "libs/examples/styled-inputs",
      "sourceRoot": "libs/examples/styled-inputs/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/examples/styled-inputs/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/examples/styled-inputs"],
          "options": {
            "jestConfig": "libs/examples/styled-inputs/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "form-builder": {
      "root": "libs/form-builder",
      "sourceRoot": "libs/form-builder/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/form-builder",
            "tsConfig": "libs/form-builder/tsconfig.lib.json",
            "project": "libs/form-builder/package.json",
            "entryFile": "libs/form-builder/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/form-builder/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/form-builder/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/form-builder"],
          "options": {
            "jestConfig": "libs/form-builder/jest.config.js",
            "passWithNoTests": true
          }
        },
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "baseBranch": "master",
            "push": true,
            "noVerify": true,
            "commitMessageFormat": "release(${projectName}): publish version ${version}",
            "postTargets": ["form-builder:publish", "form-builder:github"]
          }
        },
        "github": {
          "executor": "@jscutlery/semver:github",
          "options": {
            "branch": "master",
            "tag": "${tag}",
            "notesFile": "libs/form-builder/CHANGELOG.md"
          }
        },
        "publish": {
          "executor": "ngx-deploy-npm:deploy",
          "options": {
            "baseBranch": "master",
            "access": "public"
          }
        }
      }
    },
